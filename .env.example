# Variables de entorno
STAGE=dev
AWS_REGION=us-east-1
AWS_PROFILE=default

# Configuración de ElasticSearch
ES_USERNAME=admin
ES_PASSWORD=admin123
ES_PORT=9200
ES_TRANSPORT_PORT=9300
KIBANA_PORT=5601
LOGSTASH_PORT=5000

# Configuración de Tenants
TENANT_IDS=tenant1,tenant2,tenant3

# Configuración de AWS
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key

# Configuración de Base de Datos
ATHENA_DATABASE=cloudpf_dev
ATHENA_OUTPUT_LOCATION=s3://cloudpf-athena-results-dev/

# Configuración de S3
S3_BUCKET=cloudpf-data-lake-dev
S3_REGION=us-east-1

# Configuración de desarrollo
NODE_ENV=development
LOG_LEVEL=debug

# Configuración de puertos locales
LOCAL_ES_PORT=9200
LOCAL_KIBANA_PORT=5601

# Configuración de contenedores Docker
DOCKER_COMPOSE_PROJECT_NAME=cloudpf-ingesta
