    "create:tables": "node scripts/create-dynamodb-tables.js"
  },
  "dependencies": {
    "aws-sdk": "^2.1490.0",
    "uuid": "^9.0.1",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "serverless": "^3.38.0",
    "serverless-plugin-common-excludes": "^4.0.0",
    "serverless-plugin-include-dependencies": "^5.1.0"
  },
  "keywords": [
    "aws",
    "lambda",
    "dynamodb",
    "cdc",
    "real-time",
    "streams",
    "s3",
    "academy"
