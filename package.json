{
  "name": "cloudpf-ingesta-tiempo-real",
  "version": "1.0.0",
  "description": "Sistema CDC con DynamoDB Streams, Lambda y S3 para AWS Academy",
  "main": "index.js",
  "scripts": {
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "remove": "serverless remove",
    "logs:product": "serverless logs -f productStreamProcessor",
    "logs:purchase": "serverless logs -f purchaseStreamProcessor",
    "setup:infrastructure": "node scripts/setup-cdc-infrastructure.js",
    "test:cdc": "node scripts/test-cdc.js",
